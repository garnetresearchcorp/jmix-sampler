<window xmlns="http://jmix.io/schema/ui/window">
    <layout spacing="true">
        <label id="dateLabel">
            <formatter>
                <date type="DATE"/>
            </formatter>
        </label>
        <label id="numberLabel">
            <formatter>
                <number format="#,###"/>
            </formatter>
        </label>
    </layout>
</window>
